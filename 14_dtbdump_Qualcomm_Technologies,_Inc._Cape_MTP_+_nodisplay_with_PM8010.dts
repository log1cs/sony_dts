                cooling-device = <&display_fps 0x4 0x4>;
                trip = <&display_test_config4>;
            };
            gpu_cdev {
                cooling-device = <&msm_gpu 0x7 0xFFFFFFFF>;
                trip = <&xo_config0>;
            };
            lte_cdev {
                cooling-device = <&modem_lte_dsc 0xFF 0xFF>;
                trip = <&xo_config1>;
            };
            nr_cdev {
                cooling-device = <&modem_nr_scg_dsc 0xFF 0xFF>;
                trip = <&xo_config1>;
            };
        };
    };
};

