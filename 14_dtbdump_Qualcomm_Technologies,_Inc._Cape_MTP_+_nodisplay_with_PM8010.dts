&qupv3_se4_spi {
    #address-cells = <0x1>;
    #size-cells = <0x0>;
    qcom,spi-touch-active = "focaltech,fts_ts";
    status = "ok";
    focaltech@0 {
        compatible = "focaltech,fts_ts";
        focaltech,display-coords = <0x0 0x0 0x438 0x924>;
        focaltech,ic-type = <0x3658D488>;
        focaltech,irq-gpio = <&tlmm 0x15 0x2008>;
        focaltech,max-touch-number = <0x5>;
        focaltech,reset-gpio = <&tlmm 0x14 0x0>;
        focaltech,touch-type = "primary";
        interrupt-parent = <&tlmm>;
        interrupts = <0x15 0x2008>;
        pinctrl-0 = <&ts_active>;
        pinctrl-1 = <&ts_int_suspend &ts_reset_suspend>;
        pinctrl-2 = <&ts_release>;
        pinctrl-names = "pmx_ts_active", "pmx_ts_suspend", "pmx_ts_release";
        reg = <0x0>;
        spi-max-frequency = <0x5B8D80>;
        vdd-supply = <&L3C>;
    };
};

&qupv3_se5_i2c {
    #address-cells = <0x1>;
    #size-cells = <0x0>;
    status = "ok";
    nxp_eusb2_repeater: eusb2_repeater@4f {
        compatible = "nxp,eusb2-repeater";
